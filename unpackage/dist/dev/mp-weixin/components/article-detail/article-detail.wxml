<view class="article"><view class="article-title">{{title}}</view><view class="article-header"><view class="article-header-logo"><image src="{{article.avatar}}" mode="aspectFill"></image></view><view class="article-header-content"><view class="article-header-content-title">{{''+author_name+''}}</view><view class="article-header-content-info"><text>{{$root.f0}}</text><view><text>{{"浏览: "+browse_count}}</text><text>{{"点赞: "+thumbs_up_count}}</text></view></view></view><button class="article-author-follow" type="default" data-event-opts="{{[['tap',[['follow',['$0'],['article.author_id']]]]]}}" bindtap="__e">{{is_author_like?'取消关注':'关注'}}</button></view><view class="article-content"><view class="article-content-html"><u-parse vue-id="65d4b97c-1" content="{{content}}" noData="{{noData}}" bind:__l="__l"></u-parse></view><view class="comment"><view class="comment-title">最新评论</view><view class="comment-content"><block wx:for="{{commentList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="comment_id"><view><comments-box vue-id="{{'65d4b97c-2-'+__i0__}}" comments="{{item}}" data-event-opts="{{[['^reply',[['reply']]]]}}" bind:reply="__e" bind:__l="__l"></comments-box><view class="underline"></view></view></block></view></view></view><view class="article-action"><view data-event-opts="{{[['tap',[['openCommentView',[0]]]]]}}" class="article-action-comment-input" bindtap="__e"><text>评论</text><uni-icons vue-id="65d4b97c-3" type="compose" size="16" color="#f07373" bind:__l="__l"></uni-icons></view><view class="article-action-icons"><view data-event-opts="{{[['tap',[['likeTap',['$0'],['article._id']]]]]}}" class="article-action-icons-box" bindtap="__e"><uni-icons vue-id="65d4b97c-4" type="{{is_like?'heart-filled':'heart'}}" size="22" color="#f07373" bind:__l="__l"></uni-icons></view><view data-event-opts="{{[['tap',[['thumbsup',['$0'],['article._id']]]]]}}" class="article-action-icons-box" bindtap="__e"><uni-icons vue-id="65d4b97c-5" type="{{is_thumbs_up?'hand-thumbsup-filled':'hand-thumbsup'}}" size="22" color="#f07373" bind:__l="__l"></uni-icons></view></view></view><action-bottom class="vue-ref" bind:publish="__e" vue-id="65d4b97c-6" data-ref="popup" data-event-opts="{{[['^publish',[['publish']]]]}}" bind:__l="__l"></action-bottom></view>